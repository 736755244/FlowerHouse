<view class="section" style="height:{{Height}}px;overflow-y:scroll;">
  <view class="flex_con" wx:for="{{listinfo}}" wx:key="*this">
    <div class="i_img">
      <image src="{{item.avator}}"></image>
    </div>
    <div class="i_con">
      <text>{{item.name}}\n</text>
      <text>{{item.desc_sub}}</text>
    </div>
    <div class="i_more" bindtap="toMoreInfo" id="{{item._id}}">
      <image src="{{iconpath}}phone.png"></image>
      <image src="{{iconpath}}right.png"></image>
    </div>
  </view>
</view>