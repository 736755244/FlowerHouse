.section{
  padding: 10px;
  background: #eee;
}
.flex_con{
  height: 75px;
  display: flex;
  align-items: center;
  background: #fff;
  margin-bottom: 10px;
}
.i_img{
  margin: 10px;
  width:52px;
  height: 52px;
  vertical-align: middle;
}
.i_img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.i_con{
  width: 50%;
  margin-right: 10%;
}
.i_con >text:first-child{
  font-size: 14px;
  color: #333;
}
.i_con >text:nth-child(2){
  font-size: 12px;
  color: #888;
}
.i_more{
  display: flex;
}
.i_more image{
  width: 18px;
  height: 18px;
  margin-right: 15px;
}