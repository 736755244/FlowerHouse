.section{
  padding: 20px;
}
.header{
  font-size: 16px;
  font-weight: bold;
  display: flex;
}
.header>text{
  width: 90%;
}
.header image{
  width: 25px;
  height: 25px;
}
.img_item{
  width: 100%;
  border-radius: 5%;
  margin:10px 0;
}
.img_item image{
  width: 100%;
  height: 200px;
}
/*  */
.footer_flex{
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 49px;
  border-top: 1px solid #eee;
}
.footer_flex >div{
  width: 20%;
  font-size: 12px;
  color: #888;
  text-align: center;
}
.footer_flex >div image{
  width: 20px;
  height: 20px;
}
.footer_flex button{
  width: 50%;
  background: #F5B43E;
  color: #fff;
  height: 40px;
  line-height: 40px;
  border-radius: 20px;
  font-size: 13px;
}